{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "member-access": [true, "no-public"],
        "no-bitwise": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
    },
    "rulesDirectory": []
}
